{
	"name": "akeeba-backup",
	"description": "Akeeba Backup for Joomla!",
	"devDependencies": {
		"@babel/cli": "^7.11.6",
		"@babel/core": "^7.11.6",
		"@babel/preset-env": "^7.11.5",
		"babel-preset-minify": "^0.5.1",
		"sass": "^1.32.8"
	},
	"babel": {
		"presets": [
			[
				"@babel/preset-env",
				{
					"useBuiltIns": "entry",
					"corejs": {
						"version": "3.9",
						"proposals": true
					}
				}
			],
			[
				"minify",
				{
					"builtIns": false,
					"removeConsole": false
				}
			]
		],
		"comments": false,
		"ignore": [
			"component/media/js/*.min.js"
		]
	},
	"dependencies": {
		"core-js": "^3.9.1"
	}
}
